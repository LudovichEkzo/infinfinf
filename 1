from math import *
x=2 ; y=2 ; z=1
r=(z**x)**(2*y) + (2**y)**(2*x)
print("r = ",r)
x1=1; z1=3
r1=3*cos(x1/2)**(2) + (e**(-x1) + 2*(z1)**(2))**(1/2)
print("r1 = ",r1)
x2=6
r2=(x2**2)/(2*3*4) + ((x2-4)**(1/7))/sin(x2+1)**2
print("r2 = ",r2)
x3=4
r3=x3 + (x3**(2) + (log(x3)**2 + 0.3))/x3 + ((x3**2)/(x3+1))
print("r3 = ",r3)
